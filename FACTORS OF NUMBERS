def find_factors(number):
    factors = []
    
    for i in range(1, number + 1):
        if number % i == 0:
            factors.append(i)
    
    return factors

# Example usage
number = 25
factors = find_factors(number)

print("Factors:", end=" ")
for factor in factors:
    print(factor, end=",")

print("\n{} Factors".format(len(factors)))
